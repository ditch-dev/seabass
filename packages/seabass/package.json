{
  "name": "@seabass-ui/seabass",
  "description": "React Native primitive UI to be used in pacific-dev projects.",
  "version": "0.1.0",
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native",
    "ios",
    "android"
  ],
  "license": "EPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pacific-dev/seabass.git",
    "directory": "packages/seabass"
  },
  "bugs": {
    "url": "https://github.com/pacific-dev/seabass/issues"
  },
  "main": "lib/commonjs/index.js",
