    "react": "~16.13.1",
    "react-native": "~0.63.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
